{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": [
        "./*"
      ]
    },
    "types": [
      "node",
      "./types/env.d.ts"
    ],
    "esModuleInterop": true
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "exponpm install expo-splash-screen --save-env.d.ts",
    "types/env.d.ts",
    "expo-env.d.ts"
  ]
}
